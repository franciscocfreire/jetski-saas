{
  "dashboard": {
    "title": "Jetski SaaS - Business Metrics",
    "tags": ["jetski", "business"],
    "timezone": "browser",
    "refresh": "1m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Total Rentals (Check-ins)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(jetski_rental_checkin_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Total Check-outs",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(jetski_rental_checkout_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 3,
        "title": "Total Reservations",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(jetski_reservation_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 4,
        "title": "Reservation Cancellations",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(jetski_reservation_cancelled_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Check-in Rate (per hour)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(jetski_rental_checkin_total[1h])",
            "legendFormat": "Tenant: {{tenant_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Check-ins/hour"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Reservation Rate (per hour)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(jetski_reservation_total[1h])",
            "legendFormat": "Tenant: {{tenant_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Reservations/hour"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Rental Duration Distribution (minutes)",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 12, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(jetski_rental_duration_seconds_bucket[5m])) by (le)",
            "legendFormat": "{{le}}",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "yAxis": {
          "format": "s"
        },
        "dataFormat": "tsbuckets"
      },
      {
        "id": 8,
        "title": "Authentication - Login Success Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(jetski_auth_login_success_total[5m])",
            "legendFormat": "Successful Logins",
            "refId": "A"
          },
          {
            "expr": "rate(jetski_auth_login_failure_total[5m])",
            "legendFormat": "Failed Logins",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Logins/s"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "OPA Authorization Decisions",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (decision) (rate(jetski_opa_decision_total[5m]))",
            "legendFormat": "Decision: {{decision}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Decisions/s"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "OPA Decision Latency (P95)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(jetski_opa_decision_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95 Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(jetski_opa_decision_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P50 Latency",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Tenant Context Switches",
        "type": "stat",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(jetski_tenant_context_switch_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      }
    ]
  },
  "overwrite": true
}
